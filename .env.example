# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Add your service role key here to bypass RLS (for admin dashboard)
# WARNING: Keep this secret and never expose it in client-side code
VITE_SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Authentication Configuration
VITE_AUTH_REMEMBER_SESSION=true
VITE_AUTH_SESSION_EXPIRY=604800 # 7 days in seconds

# App Configuration
VITE_APP_NAME="Modern Admin" 